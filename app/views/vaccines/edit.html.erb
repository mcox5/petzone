<div class="container">
  <div class="row justify-content-center">
    <div class="card editar col-12 m-5 p-3 rounded-4">
      <%= simple_form_for(@vaccine) do |f| %>
        <%= f.input :name, label:"Nombre",placeholder:"Rabia canina" %>
        <%= f.input :interval,label:"Tiempo entre aplicaciones",placeholder:"Tiempo en días" %>
        <%= f.input :date,label:"Fecha de aplicación", html5:true %>
        <%= f.submit "Agregar", class:"btn btn-outline-success"%>
      <% end %>
    </div>
  </div>
</div>
