<div class="container">
  <div class="row justify-content-center">
    <div class="card editar col-12 m-5 p-3 rounded-4">
      <%= simple_form_for([@pet, @exam]) do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :date %>
        <%= f.input :photos, as: :file, input_html: {multiple: true} %>
        <%= f.submit "Guardar examenes", class:"btn btn-outline-success"%>
      <% end %>
    </div>
  </div>
</div>
